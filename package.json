{
	"name": "vscode-splunk-search-linter",
	"displayName": "vscode-splunk-search-linter",
	"description": "Splunk Search Linter",
	"version": "0.2.0",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:splunk_search"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/arcsector/vscode-splunk-search-linter.git"
	},
	"icon": "assets/icon.png",
	"galleryBanner": {
		"color": "#1e1e1e",
		"theme": "dark"
	},
	"main": "./out/extension.js",
	"scripts": {
